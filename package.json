    "form-data": "^4.0.4",
    "haversine-distance": "^1.2.4",
    "i18n-iso-countries": "^7.14.0",
    "next": "^15.3.4",
    "next-auth": "^4.24.11",
    "node-cron": "^4.2.0",
    "pg-boss": "^12.1.1",
    "prisma": "^6.11.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "swr": "^2.3.4",
    "zod": "^3.25.72"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",

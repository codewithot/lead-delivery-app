  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build && npm run build:workers",
    "build:workers": "tsc src/workers/standalone.ts --outDir dist --esModuleInterop --resolveJsonModule --skipLibCheck",
    "start": "next start",
    "lint": "next lint",
